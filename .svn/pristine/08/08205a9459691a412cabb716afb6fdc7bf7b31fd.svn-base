<template>
  <div class="content">
    <components
      v-if="this.chartType !== 'table' && this.queryData"
      :is="this.chartType"
      :settings="this.settings"
      :dataTable="queryData.data"
      :active="this.active"
    />
    <slot v-else name="empty"/>
  </div>
</template>

<script>
import { components } from "@person/components/chart";
export default {
  props: {
    queryData:{//图形数据
      type: Object,
    },
    chartType:{//图形类型
      type: String,
    },
    settings: {//配置项
      type: Object,
    },
    active: {//左侧菜单选中下标
      type: Number,
    }
  },
  components: {
    ...components,
  },
  data() {
    return {};
  },
};
</script>
