<template>
  <div class="report-detail">
    <div class="main">
      <div class="main-top">
        <a-button>一键导出</a-button> 
      </div>
      <div class="main-content">
        <Content v-if="detailData" :content="detailData" />
      </div>
    </div>
  </div>
</template>
<script>
import { Button } from "ant-design-vue";
import { reportDetail } from "@/person-shaoxing/api/assessment";
import Content from "./components/Content";
import { showError } from "@/framework/utils/index";
import Bus from '@/framework/utils/EventBus';

export default {
  components: {
    AButton: Button,
    Content
  },
  data() {
    return {
      detailData: undefined,
    }
  },
  created() {
    if(this.$route.query.id) {
      this.getData();
    }
  },
  methods: {
    getData() {
      reportDetail(this.$route.query.id)
      .then(({result}) => {
        // this.detailData = result;
        this.detailData = {
          baseInfo: undefined,
          staff: undefined,
        }
      })  
      .catch(err => {
        showError(err);
      })
    }
  }
}
</script>
<style lang="less" scoped>
.report-detail{
  padding: 10px;
  height: 100%;
  .main{
    height: 100%;
    background: @white;
    .main-top{
      padding: 18px @content-padding-h 8px;
    }
  }
}
</style>