/**
 * 人员调动列表查询 
 * data对象属性
 * @param {int} nodeid 组织节点id
 * @param {enum} type  1: 调入   2:调出
 * @param {String} searchkey 查询字符串
 * @param {int} nodeid 组织节点id
 * @param {String} starttime 开始时间
 * @param {String} endtime 结束时间
 */
export function transferList (data) {
  return request({
    url: '/hall2/personneltransfer/query',
    method: 'post',
    data: data
  })
}
