import request from '@/framework/utils/request'

/**
 * 获取考核概况
 * @param {int} nodeid 组织节点id
 */
export function exam (nodeid) {
  return request({
    url: '/hall2/v2/userreport/getisneedassessmap',
    method: 'post',
    data: { nodeid }
  })
}

