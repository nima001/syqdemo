import request from '@/framework/utils/request'

export function mergeOrg(data){
  return request({
      url:'/idm/v2/org/mergeorg',
      method:'post',
      data
  })
}

export function improveOrginfo(data){
  return request({
      url:'/idm/v2/org/improveOrginfo',
      method:'post',
      data
  })
}