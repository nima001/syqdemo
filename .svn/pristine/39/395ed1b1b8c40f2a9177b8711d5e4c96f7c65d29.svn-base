<template>
  <div class="org-listednumber">
    <a-row :gutter="20">
      <a-col :span="24">
        <span class="name">机构挂牌情况：{{data}}，{{attachname}}</span>
      </a-col>
    </a-row>
  </div>
</template>

<script>
/*
**机构挂牌情况
*/
import { Row, Col } from "ant-design-vue";
export default {
  props: {
    data: String
  },
  components: {
    ARow: Row,
    ACol: Col,
  },
  data() {
    return {
    };
  },
  watch: {},
  computed: {
    attachname() {
      if(this.data) {
        if(this.data.indexOf("、") != -1) {
          return "挂牌超过2个及以上"
        }else{
          return "挂牌没超过2个"
        }
      }else{
        return '否'
      }
    }
  },
  created() {},
  mounted() {},
  methods: {},
};
</script>
<style lang="less" scoped>
.org-listednumber{  
  /deep/.ant-row{
    padding: @content-padding-v 0;
  }
}
</style>