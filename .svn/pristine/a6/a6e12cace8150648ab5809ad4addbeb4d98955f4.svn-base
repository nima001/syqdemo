import request from "../utils/request"

export function indexReportdata() {
  return request({
      url: `/idmgzmh/index/report/indexReportdata`,
      method: 'get'
  })
}

export function loginDate(data) {
  return request({
      url: `/idmgzmh/index/report/loginDate`,
      method: 'post',
      data
  })
}

export function realTimeData() {
  return request({
      url: `/idmgzmh/index/report/realTimeData`,
      method: 'get'
  })
}

export function getMenu() {
  return request({
      url: `/idmgzmh/acl/menu/getMenuData`,
      method: 'get'
  })
}

//  是否第一次登陆
export function isFristLogin() {
  return request({
      url: `/idmgzmh/index/isFirstLogin`,
      method: 'get'
  })
}

// 获取菜单
export function getListMenu() {

		return request({
      url: `/idmgzmh/acl/menu/getMenuData`,
      method: 'get'
  	});

}


