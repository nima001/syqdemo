import request from '@/framework/utils/request'

export function separateOrg(data){
  return request({
      url:'/idm/v2/org/separateorg',
      method:'post',
      data
  })
}

export function completeOrginfo(data){
  return request({
      url:'/idm/v2/org/completeOrginfo',
      method:'post',
      data
  })
}

export function moveOrg(data){
  return request({
      url:'/idm/v2/org/moveorg',
      method:'post',
      data
  })
}