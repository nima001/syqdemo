
export default {
	icon: 'chart-pie',
  props: {
		data: {// 数据
      type: Object,
    },
    settings: {// 配置
      type: Object,
      default: () => ({})
    },
	},
	computed: {
		title(){
			let { title, context = {}} = this.settings;
			if(title){
				for(let key in context){
					title = title.replace(new RegExp('\\$\\{' + key + '\\}', 'g'), context[key]);
				}
			}
			return title;
		}
	}
}