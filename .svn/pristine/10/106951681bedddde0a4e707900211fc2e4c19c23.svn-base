<template>
  <div class="container">
    <a-input-number v-model="data1" @change="handleChange" style="width:50%;float:left;"/>
    <div style="width:50%;padding-left:10px;float:left;">
      <a-input-number v-model="data2" @change="handleChangeTwo" style="width:100%"/>
    </div>
  </div>
</template>
<script>
import { InputNumber } from "ant-design-vue";
export default {
  name: "TypeThree",
  props: {
    position: {
      type: String,
      required: true
    },
    defaultData: {
