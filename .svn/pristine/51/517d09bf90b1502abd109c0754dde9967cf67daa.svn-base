<template>
  <div class="school-leader">
    <a-row :gutter="20">
      <a-col :span="24">
        <a-table 
          class="rule-table"
          :rowKey="generateRowKey"
          :columns="columns" 
          :dataSource="tableData" 
          :pagination="false"
          :bordered="true"
          >
        </a-table>
      </a-col>
    </a-row>
  </div>
</template>

<script>
/*
**学校领导职数配置规则
*/
import { Row, Col, Table } from "ant-design-vue";
import { createReportItem } from "@/person-shaoxing/utils/index";
export default {
  props: {
    data: Object
  },
  components: { ARow: Row, ACol: Col, ATable: Table },
  data() {
    return {
      columns: [
        { title: '全日制在校生', dataIndex: 'studennum', width: '30%' },
        { title: '领导职数', dataIndex: 'leadernum', width: '30%' },
        { title: '操作', dataIndex: 'ps', width: '40%' }
      ],
      tableData: []
    };
  },
  watch: {},
  computed: {},
  created() {
    this.tableData = [this.data];
  },
  mounted() {
    let data = {}
    data.isSort = false;
    data.isSpecial = false;
    data.tableData = this.tableData;
    data.headArr = [
      {text: '全日制在校生', colspan: 1}, 
      {text: '领导职数', colspan: 1}, 
      {text: '操作', colspan: 1}
    ];
    this.$store.commit('setProcessedData', { key: this.str, data});
  },
  methods: {
    getHtml() {
      let thead = `<thead><tr>`
      let tbody = `<tbody>`
      const headArr = [
        {text: '全日制在校生', colspan: 1}, 
        {text: '领导职数', colspan: 1}, 
        {text: '操作', colspan: 1}
      ]
      //  生成表头
      headArr.forEach(item => {
        if (item.colspan) {
          thead += `<td colspan="${item.colspan}">${item.text}</td>`
        } else {
          thead += ''
        }
      })
      thead += '</tr></thead>'
      //  生成内容
      let width = Math.round(1/headArr.length * 10000) / 100 + '%';
      this.tableData.forEach((item, index) => {
        tbody += '<tr>'
        headArr.forEach((list, num) => {
          if (num == 0) {
            tbody += `<td style="width: ${width};" rowspan="1">${item.allowNum}</td>`
          } else if(num == 1) {
            tbody += `<td style="width: ${width};" rowspan="1">${item.useNum}</td>`
          } else if(num == 2) {
            tbody += `<td style="width: ${width};" rowspan="1">${item.averageInternalCopileNum}</td>`
          } else {
            tbody += `<td style="width: ${width};" rowspan="1">${item.suggestNum}</td>`
          }
        })
        tbody += '</tr>'
      })
      tbody += `</tbody>`
      let temp = `<table style="width: 100%;">${thead}${tbody}</table>`
      return createReportItem(temp, '关联情况分析')
    },
    generateRowKey() {
      return Number(Math.random().toString().substr(3, 3) + Date.now()).toString(36)
    },
  },
};
</script>
<style lang="less" scoped>
.school-leader{
  /deep/.ant-row{
    padding: @content-padding-v 0;
  }
}
</style>