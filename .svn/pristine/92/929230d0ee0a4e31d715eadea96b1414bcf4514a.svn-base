/**
 * 实名制模块路由
 */
import Layout from '../../views/Layout'

export const orgRoutes = {
  path: '/person/org',
  component: Layout,
  children: [
    {
      path: '',
      component: () => import('../../views/org/index'),
      name: 'orgIndex',
      redirect: 'index',
      children: [
        {
          path: 'index',
          component: () => import('../../views/org/Main'),
          name: 'orgMain',
          meta: {
            auth: 'login',
            path: [{ name: '实名制', path: '/person/org' }]
          }
        }
      ]
    }
  ]
}
