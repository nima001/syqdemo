import Vue from 'vue'
import Router from 'vue-router'

Vue.use(Router)

/* Router Modules */
import { workflowDesigna } from './modules/flowdesign';
import { modelApplicationManage } from './modules/modelapplication';

export default new Router({
  mode: 'history',
  base: process.env.BASE_URL,
  routes: [
    workflowDesigna,
    modelApplicationManage,
    {
      path: '*',
      component: () => import('@framework/views/error/404'),
      meta: { title: '404' }
    }
  ]
});