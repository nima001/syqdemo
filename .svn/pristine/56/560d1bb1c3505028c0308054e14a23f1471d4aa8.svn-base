/**
 * 管理中心路由
 */
import Layout from '../../views/Layout'
export const manageCenter = {
  path: '/dev/manage',
  component: Layout,
  children: [
    {
      path: 'index',
      component: () => import('@/dev/views/ManagementCenter'),
      meta: { access: 'login' }
    },
    {
      path: 'create',
      component: () => import('@/dev/views/app/AppAdd'),
      meta: {
        path: [{ name: '应用中心', path: '/dev/manage/index' },{ name: '创建应用', path: '/dev/manage/create' }],
        access: 'auth'
      }
    }
  ]
}
