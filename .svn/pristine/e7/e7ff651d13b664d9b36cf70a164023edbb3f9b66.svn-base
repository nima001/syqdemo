<script>
export default {
  data() {
    return {
      dictId: "",
      timerId: null
    };
  },
  computed: {
    dickList() {
      let arr = [""];
      let list = this.$store.getters.dict("usermanage.org.district");
      if (list) {
        list.forEach(item => {
          if (item.value != "330600") {
            arr.push(item.value);
          }
        });
      }
      return arr;
    }
  },
  mounted() {
    this.$emit("dictId", this.dictId);
    this.loop();
  },
  beforeDestroy() {
    if (this.timerId) {
      clearInterval(this.timerId);
    }
  },
  methods: {
    loop() {
      this.timerId = setInterval(() => {
        setTimeout(() => {
          let index = this.dickList.indexOf(this.dictId);
          if (index < this.dickList.length - 1) {
            this.dictId = this.dickList[index + 1];
          } else {
            this.dictId = this.dickList[0];
          }
          this.$emit("dictId", this.dictId);
        }, 0);
      }, 2000);
    },
    svgMove(e) {
      let target = e.target;
      if (target.nodeName == "path" || target.nodeName == "DIV" || target.nodeName == "SPAN") {
        clearInterval(this.timerId);
        if (target.id) {
          this.dictId = target.id;
        } else {
          this.dictId = "";
        }
        this.$emit("dictId", this.dictId);
      } else {
        if (this.timerId) {
          clearInterval(this.timerId);
        }
        this.loop();
      }
    },
    leave() {
      if (this.timerId) {
        clearInterval(this.timerId);
      }
      this.loop();
    }
  }
};
</script>
